<h1>Add a New Concert<h1>

	<%= form_for @the_concert do |f| %>

		<%= f.label :artist  %>
		<%= f.text_field :artist %>

		<br>

		<%= f.label :venu  %>
		<%= f.text_field :venu %>

		<br>

		<%= f.label :city  %>
		<%= f.text_field :city %>

		<br>

		<%= f.label :date  %>
		<%= f.text_field :date %>

		<br>

		<%= f.label :price  %>
		<%= f.text_field :price %>

		<br>

		<%= f.label :description %>

		<br>
		
		<%= f.text_area :description %>

		<br>

		<%= f.button %>

	<% end %>